{
  "name": "bill-split",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.1.1",
    "@material-ui/icons": "^4.2.0",
    "@types/enzyme": "^3.9.3",
    "@types/jest": "24.0.14",
    "@types/node": "12.0.8",
    "@types/react": "16.8.20",
    "@types/react-dom": "16.8.4",
    "@types/react-redux": "^7.1.0",
    "@types/react-router-dom": "^4.3.4",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "redux": "^4.0.1",
    "serve": "^11.0.1",
    "ts-money": "^0.4.6",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "3.5.2"
  },
  "scripts": {
    "autopre": "yarn run lint && yarn run precommit",
    "start": "REACT_APP_VERSION=`git rev-parse --short HEAD` react-scripts start",
    "build": "CI=false react-scripts build",
    "dev": "yarn run start",
    "fix": "yarn run lint",
    "lint": "eslint --fix --ext .ts,.tsx src/",
    "precommit": "yarn run verify-lint && yarn run build && yarn run test",
    "pre": "yarn run precommit",
    "test": "react-scripts test --watchAll=false",
    "test:watch": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --forceExit --detectOpenHandles --watchAll=false",
    "test:coveralls": "cat coverage/lcov.info | node node_modules/.bin/coveralls",
    "eject": "react-scripts eject",
    "verify-lint": "eslint --ext .ts,.tsx src/"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "prettier": "^1.18.2"
  }
}
