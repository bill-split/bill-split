{
  "name": "bill-split",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "@types/enzyme": "^3.9.1",
    "@types/jest": "24.0.11",
    "@types/node": "11.13.6",
    "@types/react": "16.8.14",
    "@types/react-dom": "16.8.4",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "jest": "23.6.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "2.1.8",
    "ts-money": "^0.4.6",
    "typescript": "3.4.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "fix": "yarn run lint && yarn run tsfmt",
    "lint": "tslint --fix -c tslint.json 'src/*.ts*'",
    "precommit": "yarn run verify-lint && yarn run verify-tsfmt && yarn run test",
    "test": "react-scripts test --watchAll=false",
    "test:watch": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --forceExit --detectOpenHandles",
    "test:coveralls": "cat coverage/lcov.info | node node_modules/.bin/coveralls",
    "pre": "yarn run precommit",
    "eject": "react-scripts eject",
    "tsfmt": "tsfmt -r src/*.ts*",
    "verify-lint": "tslint -c tslint.json 'src/*.ts*'",
    "verify-tsfmt": "tsfmt --verify src/*.ts*"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "typescript-formatter": "^7.2.2"
  }
}
